{
  "name": "mobx-react-cloud",
  "version": "1.0.0",
  "description": "仿百度云盘客户端",
  "main": "./main.js",
  "scripts": {
    "start": "npm-run-all --parallel serve dev",
    "serve": "cross-env NODE_ENV=development pm2 kill && pm2 start ./server.js --no-daemon --watch",
    "dev": "cross-env NODE_ENV=development electron . --debug",
    "compile": "cross-env NODE_ENV=production && webpack",
    "compile:main": "cross-env NODE_ENV=production && webpack --config ./build/webpack.electron.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package:win": "cd package && electron-packager ./ --overwrite --platform=win32",
    "release": "cd package && electron-builder"
  },
  "author": {
    "name": "woox.wzd",
    "email": "woox.wzd@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zedwang/iCloud.git"
  },
  "build": {
    "productName": "iCloud",
    "appId": "com.zed.app",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    },
    "directories": {
      "output": "../release"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.39",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.39",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/polyfill": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-es2015": "^7.0.0-beta.39",
    "@babel/preset-react": "^7.0.0-beta.39",
    "@babel/register": "^7.0.0-beta.39",
    "autoprefixer": "^7.2.6",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.0.0-beta.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "debug": "^3.1.0",
    "electron": "^1.8.2",
    "electron-builder": "^19.56.0",
    "electron-json-storage": "^4.0.2",
    "electron-packager": "^11.1.0",
    "electron-updater": "^2.20.1",
    "electron-webpack": "^1.12.1",
    "electron-window-state": "^4.1.1",
    "express-http-proxy": "^1.1.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.18.0",
    "iconv-lite": "^0.4.19",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "pm2": "^2.10.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "2.0.6",
    "webpack-dev-server": "2.11.1",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "immutability-helper": "^2.6.4",
    "mobx": "^3.5.1",
    "mobx-react": "^4.4.1",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.1",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  }
}
